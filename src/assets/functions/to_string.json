{
  "title": "TO_STRING",
  "description": "<p>Converts an object into an unformatted text string.</p>",
  "syntax": "TO_STRING(obj) -> String",
  "parameters": [
    {
      "name": "obj",
      "type": "Object",
      "required": "Yes",
      "description": "The object to convert."
    }
  ],
  "examples": [
    "TO_STRING(0.25)\n-- Returns \"0.25\"",
    "TO_STRING(\"2005-11-15\")\n-- Returns \"2005-11-15\""
  ],
  "tips": [
    {
      "text": "<p>The returned text is unformatted; it does not include currency symbols, percent signs, or commas.</p>",
      "children": []
    },
    {
      "text": "<p>Values are not locale-sensitive (e.g., 24.42 EUR becomes \"24.42\").</p>",
      "children": []
    },
    {
      "text": "<p>Percent values are returned in decimal form.</p>",
      "children": []
    },
    {
      "text": "<p>Dates are returned as YYYY-MM-DD.</p>",
      "children": []
    },
    {
      "text": "<p>Datetime values are returned as YYYY-MM-DD hh:mm:ss.</p>",
      "children": []
    }
  ],
  "relatedFormulas": []
}